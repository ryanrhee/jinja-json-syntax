{
    "name": "jinja-json-syntax",
    "displayName": "jinja-json-syntax",
    "description": "Jinja with JSON syntax file",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/ryanrhee/jinja-json-syntax"
    },
    "version": "0.0.3",
    "publisher": "ryanrhee",
    "engines": {
        "vscode": "^1.0.0"
    },
    "categories": [
        "Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "jinjajson",
                "aliases": [
                    "Jinja JSON",
                    "jinjajson"
                ],
                "extensions": [
                    ".json"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "jinjajson",
                "scopeName": "text.json.jinja",
                "path": "./syntaxes/jinjajson.tmLanguage.json"
            }
        ]
    }
}